fun makeFilter(min) {
  fun filter(n) {
    if (n < min) {
      return false;
    }
    return true;
  }
  return filter;
}

// This function applies a function to a list of
// numbers
fun applyToNumbers(f, count) {
  var n = 0;
  while (n < count) {
    if (f(n)) {
      print n;
    }
    n = n + 1;
  }
}

var greaterThanX = makeFilter(82);
var greaterThanY = makeFilter(41);

print "Numbers >= 82:";
applyToNumbers(greaterThanX, 82 + 5);

print "Numbers >= 41:";
applyToNumbers(greaterThanY, 41 + 5);
